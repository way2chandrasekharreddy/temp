package com.fisglobal.CTI_TransferData.model;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.NotNull;

@Getter
@Setter
@ToString
public class CTITransferDataRequest {

    @NotBlank(message="ivrInteractionId cannot be blank")
    @NotNull(message="ivrInteractionId cannot be null")
    private String ivrInteractionId;

    private String acdInteractionId;

    private MerchantAuthJson merchantAuthJson;

    private CTIDataJson ctiDataJson;

    private String targetNLPInteractionId;

    private String iscCode;

    private String createdBy;

}
